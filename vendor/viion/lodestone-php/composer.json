{
    "name": "viion/lodestone-php",
    "type": "library",
    "keywords": ["ffxiv","php","parser","lodestone"],
    "description": "Small lodestone parser built in PHP. Relys on the XIVDB API for some data conversion.",
    "homepage": "https://github.com/viion/lodestone-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Freeman",
            "email": "josh@viion.co.uk",
            "homepage": "http://xivdb.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/css-selector": ">=3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-0": {
            "Lodestone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Lodestone\\Tests\\": "tests/"
        }
    }
}
